{
  "manifest_version": 2,

  "name": "typing logger",
  "description": "logs typing activity and computest stats",
  "version": "1.0",


  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["libs/jquery-2.1.0.min.js", "logger.js"]
    }
  ],
  "permissions": [
    "https://secure.flickr.com/",
    "http://www.google.com/",
    "http://localhost/*",

    "storage"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
